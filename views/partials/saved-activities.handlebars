<div class="container" id="saved-activities-container">
   {{#if groupedActivities}}
  <div class="row" id="saved-activities-headline">
    <h1 class="my-saved-parks">
      MY SAVED PARKS
    </h1>
  </div>
 {{#each groupedActivities}}
  <div class="park-container-1">
    <div class="row" class="saved-park-headline">
      <h2 class="park-name">
        {{@key}}
      </h2>
    </div>
    <div class="park-container">
      {{#each this}}
      <div id="{{this.id_}}" class="row saved-activity-card">
        <div class="col col-2 saved-activity-image-div">
          <img src="{{this.image}}"
            class="saved-activity-image" alt="{{this.title}}">
        </div>

        <div class="col col-9 saved-activities-title"><a href="{{this.url}}" class="saved-activity-link" >
          {{this.title}}
        </a></div>
        <div class="col col-1 delete-button-div">
            <form class="delete-btn-form">
            <input type="hidden" name="id" value="{{this.id}}">
            <button type="submit" class="delete-btn btn btn-success btn-sm" data-activity-id="{{this.id}}">Delete</button>
          </form>
        </div>
      </div>
      {{/each}}
    </div>

  </div>
      {{/each}}

  {{else}}
  {{#if error}}
  {{error}}
  {{/if}}
  {{/if}}
  
</div>

 <script src="/js/savedActivities.js"></script>